<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-3.xsd">
    <entity entity-name="ExternalOrderSyncHistory" package="co.hotwax.integration">
        <field name="orderSyncId" type="id" is-pk="true"/>
        <field name="orderId" type="id"/>
        <field name="externalSystemId" type="id"/>
        <field name="syncDate" type="date-time"/>
    </entity>

    <entity entity-name="TransferOrderItemHistory" package="co.hotwax.integration.order">
        <field name="historyId" type="id" is-pk="true"/>
        <field name="orderId" type="id"/>
        <field name="orderItemSeqId" type="id"/>
        <field name="createdDate" type="date-time"/>
        <field name="comments" type="text-medium"/>
        <relationship type="one" related="org.apache.ofbiz.order.order.OrderItem">
            <key-map field-name="orderId"/>
            <key-map field-name="orderItemSeqId"/>
        </relationship>
    </entity>
    <entity entity-name="NetsuiteInventoryReset" package="co.netsuite.integration">
        <field name="resetItemId" type="id" is-pk="true"/>
        <field name="resetDateResourceId" type="id"/>
        <field name="facilityId" type="id"/>
        <field name="productId" type="id"/>
        <field name="externalFacilityId" type="id"/>
        <field name="netsuiteProductId" type="id"/>
        <field name="inventoryItemId" type="id"/>
        <field name="inventoryItemATP" type="number-decimal"/>
        <field name="inventoryItemQOH" type="number-decimal"/>
        <field name="externalATP" type="number-decimal"/>
        <field name="externalQOH" type="number-decimal"/>
        <field name="createdDate" type="date-time"/>
        <field name="inventoryItemATPDiff" type="number-integer"/>
        <field name="inventoryItemQOHDiff" type="number-integer"/>

        <relationship type="one" fk-name="EXTINVRST_FAC" related="org.apache.ofbiz.product.facility.Facility">
            <key-map field-name="facilityId"/>
        </relationship>
        <relationship type="one" fk-name="EXTINVRST_PROD" related="org.apache.ofbiz.product.product.Product">
            <key-map field-name="productId"/>
        </relationship>
        <relationship type="one" fk-name="EXTINVRST_INVITEM" related="org.apache.ofbiz.product.inventory.InventoryItem">
            <key-map field-name="inventoryItemId"/>
        </relationship>
    </entity>

</entities>